    "pretest": "NODE_ENV=test npm run lint",
    "serve-docs": "./node_modules/.bin/gitbook serve",
    "start": "NODE_ENV=development ./node_modules/.bin/webpack --config package.config.js --watch",
    "stories": "NODE_ENV=development ./node_modules/.bin/start-storybook -c storybook -p 8082 -s ./static",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start",
    "test-watch": "NODE_ENV=test ./node_modules/.bin/karma start --no-single-run --reporters=mocha",
    "typestories": "NODE_ENV=development ./node_modules/.bin/start-storybook -c storybookDatatypes -p 8082"
