    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "custom-protocol-detection": "1.0.1",
    "fixed-data-table-2": "0.8.15",
    "github-api": "^3.3.0",
    "history": "2.1.2",
    "lodash": "4.17.15",
    "moment-timezone": "0.5.31",
    "prop-types": "15.7.2",
