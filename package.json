{
  "name": "yourloops",
  "version": "3.7.0",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D) built on top of the [Tidepool](http://tidepool.org/) platform.",
  "repository": {
    "url": "https://github.com/mdblp/blip"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bump-version": "VERSION=$VERSION npm run bump-version:workspace && npm run bump-version:server && npm run bump-version:cloudfront-dist",
    "bump-version:cloudfront-dist": "cd cloudfront-dist/deployment && npm version $VERSION --no-git-tag-version",
    "bump-version:server": "cd server && npm version $VERSION --no-git-tag-version",
    "bump-version:workspace": "npm version $VERSION --no-git-tag-version && npm version $VERSION --no-git-tag-version --workspaces",
    "mrproper": "npm run mrproper --workspaces",
    "postmrproper": "rm -rf deploy node_modules cloudfront-dist/deployment/node_modules server/node_modules",
    "clean": "npm run clean --workspaces --if-present",
    "postclean": "rm -rf dist reports",
    "copy-css": "npm run copy-files -w dumb",
    "prebuild": "rm -rf dist",
    "build": "npm run build -w medical-domain -w dumb -w yourloops",
    "postbuild": "mkdir dist && cp -r public dist && mv packages/yourloops/dist dist/static",
    "gen-robot": "node server/gen-robot-txt.js",
    "gen-sitemap": "node server/gen-sitemap-xml.js",
    "gen-lambda": "cd server && npm install && node cloudfront-gen-lambda.js",
    "lint": "eslint templates server && npm run lint-blip && npm run lint --workspaces --if-present",
    "lint-blip": "eslint packages",
    "lint-fix-blip": "npm run lint-blip -- --fix",
    "lint-yourloops": "npm run lint -w yourloops",
    "lint-fix-yourloops": "npm run lint-fix -w yourloops",
    "lint-medical-domain": "npm run lint -w medical-domain",
    "lint-fix-medical-domain": "npm run lint-fix -w medical-domain",
    "lint-dumb": "npm run lint -w dumb",
    "lint-fix-dumb": "npm run lint-fix -w dumb",
    "test": "npm run test-locales && npm run test-lambda && npm run build-dependencies && npm run test --workspaces",
    "test-dumb": "npm run test -w dumb",
    "test-lambda": "bash test-lambda.sh",
    "test-locales": "mocha --recursive test/locales",
    "test-medical-domain": "npm run test -w medical-domain",
    "test-tideline": "npm run test -w tideline",
    "test-tideline-debug": "npm run test-debug -w tideline",
    "test-yourloops": "npm run test-yourloops-integration && npm run test-yourloops-unit",
    "test-yourloops-integration": "bash -c 'source config/.env.sh && npm run integration-test -w yourloops'",
    "test-yourloops-integration-hcp": "bash -c 'source config/.env.sh && npm run integration-test-hcp -w yourloops'",
    "test-yourloops-integration-caregiver": "bash -c 'source config/.env.sh && npm run integration-test-caregiver -w yourloops'",
    "test-yourloops-integration-patient": "bash -c 'source config/.env.sh && npm run integration-test-patient -w yourloops'",
    "test-yourloops-integration-common": "bash -c 'source config/.env.sh && npm run integration-test-common -w yourloops'",
    "test-yourloops-unit": "bash -c 'source config/.env.sh && npm run unit-test -w yourloops'",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "build-dependencies": "npm run build-medical-domain && npm run build-dumb",
    "build-dumb": "npm run build -w dumb",
    "build-medical-domain": "npm run build -w medical-domain",
    "build-yourloops": "npm run build -w yourloops",
    "prebuild-dev": "rm -rf dist",
    "build-dev": "NODE_OPTIONS='--max-old-space-size=4096 --trace-deprecation' npm run build-dev -w yourloops",
    "postbuild-dev": "mkdir dist && cp -r public dist && mv packages/yourloops/dist dist/static",
    "start-dev": "npm run copy-css && bash -c 'source config/.env.sh && npm run start-dev -w yourloops'",
    "sort-translations": "node ./locales/translations-sorter.js"
  },
  "engines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-flow": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/runtime": "^7.25.0",
    "@faker-js/faker": "^8.2.0",
    "@peculiar/webcrypto": "^1.4.3",
    "@serprex/react-motion": "0.6.3",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/blob-stream": "^0.1.31",
    "@types/chai": "^4.3.9",
    "@types/d3-array": "^3.2.1",
    "@types/d3-format": "^3.0.3",
    "@types/d3-scale": "^4.0.6",
    "@types/d3-time-format": "^4.0.3",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.200",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "@types/mocha": "^10.0.2",
    "@types/pdfkit": "^0.12.12",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.13",
    "@types/react-router-dom": "^5.3.3",
    "@types/sinon": "^17.0.0",
    "@types/uuid": "^10.0.0",
    "@types/voilab-pdf-table": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.10",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "d3-time-format": "4.1.0",
    "eslint": "^8.52.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-moment-utc": "^1.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-when": "^3.6.0",
    "karma": "^6.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.2.0",
    "pdfkit": "^0.17.1",
    "sinon": "^17.0.1",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "voilab-pdf-table": "0.5.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-subresource-integrity": "^5.1.0"
  }
}
