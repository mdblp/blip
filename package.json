{
  "name": "yourloops",
  "version": "2.0.6-beta4",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D) built on top of the [Tidepool](http://tidepool.org/) platform.",
  "repository": {
    "url": "https://github.com/mdblp/blip"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "mrproper": "npm run clean && lerna run mrproper",
    "postmrproper": "rm -rf node_modules cloudfront-dist/deployment/node_modules server/node_modules",
    "clean": "lerna exec -- rm -rf dist coverage deploy reports && lerna clean --yes",
    "postclean": "rm -rf doc dist coverage",
    "info": "lerna info",
    "prebuild": "rm -rf dist",
    "build": "lerna --stream --scope yourloops run build",
    "postbuild": "mkdir dist && mv packages/yourloops/dist dist/static",
    "gen-robot": "node server/gen-robot-txt.js",
    "gen-sitemap": "node server/gen-sitemap-xml.js",
    "gen-lambda": "node server/cloudfront-gen-lambda.js",
    "lint": "eslint *.js test templates server packages",
    "clean-coverage": "rm -rf coverage reports",
    "pretest": "npm run clean-coverage",
    "test": "npm run test-locales && npm run test-sundial && npm run test-tideline && npm run test-viz && npm run test-blip && npm run test-yourloops",
    "test-sundial": "lerna --stream --scope=sundial run test",
    "test-tideline": "lerna --stream --scope=tideline run test",
    "test-tideline-debug": "lerna --stream --scope=tideline run test-debug",
    "test-viz": "lerna --stream --scope=tidepool-viz run test",
    "test-viz-debug": "lerna --stream --scope=tidepool-viz run test-debug",
    "test-blip": "lerna --stream --scope=blip run test",
    "test-blip-debug": "lerna --stream --scope=blip run test-debug",
    "test-yourloops": "bash -c 'source config/.env.sh && lerna --stream  --scope=yourloops run test'",
    "test-yourloops-debug": "bash -c 'source config/.env.sh && lerna --stream  --scope=yourloops run test-debug'",
    "test-locales": "mocha --recursive test/locales",
    "test-lambda": "bash test-lambda.sh",
    "build-yourloops": "lerna --stream --scope yourloops run build",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "prebuild-soup": "mkdir -pv doc",
    "prebuild-dev": "rm -rf dist",
    "build-dev": "NODE_OPTIONS='--max-old-space-size=4096 --trace-deprecation' lerna run --stream --scope yourloops build-dev",
    "postbuild-dev": "mkdir dist && mv packages/yourloops/dist dist/static",
    "start-dev": "bash -c 'source config/.env.sh && lerna run --stream --scope yourloops start-dev'",
    "sort-translations": "node ./locales/translations-sorter.js"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "dependencies": {
    "blip": "file:packages/blip",
    "sundial": "file:packages/sundial",
    "tideline": "file:packages/tideline",
    "tidepool-viz": "file:packages/viz",
    "yourloops": "file:packages/yourloops"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-class-static-block": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/chai": "^4.2.22",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "*",
    "@types/lodash": "^4.14.168",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "@types/mocha": "^8.2.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/sinon": "^10.0.0",
    "@types/uuid": "^8.0.0",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-moment-utc": "^1.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.4.2",
    "mocha": "^9.1.2",
    "morgan": "^1.10.0",
    "object-invariant-test-helper": "^0.1.1",
    "postcss": "^8.3.9",
    "postcss-calc": "^8.0.0",
    "postcss-custom-properties": "^12.0.0",
    "postcss-loader": "^6.1.1",
    "react-test-renderer": "^16.14.0",
    "redux-mock-store": "^1.5.4",
    "sinon": "^11.1.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-subresource-integrity": "^5.0.0"
  }
}
