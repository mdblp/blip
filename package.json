{
  "name": "blip",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "test": "TZ=UTC NODE_ENV=test karma start",
    "pretest": "NODE_ENV=test npm run lint",
    "nightwatch": "export API_HOST='http://localhost:8009' && npm run build && npm run install-selenium && nightwatch",
    "nightwatch-single": "nightwatch",
    "browser-tests": "NODE_ENV=test karma start --browsers Chrome",
    "test-watch": "TZ=UTC NODE_ENV=test karma start --no-single-run --reporters=mocha",
    "start": "NODE_ENV=development webpack-dev-server --cache --colors --progress --port 3000 --host 0.0.0.0",
    "dblp": "NODE_ENV=development API_HOST='http://localhost:8009' ALLOW_SIGNUP_PATIENT=false ALLOW_PATIENT_CHANGE_EMAIL=false I18N_ENABLED=true ALLOW_PATIENT_CHANGE_PASSWORD=false ALLOW_CHANGE_LANG=false HIDE_DONATE=true HIDE_DEXCOM_BANNER=true HIDE_UPLOAD_LINK=true BRANDING='diabeloop' webpack-dev-server --cache --colors --progress --port 3000 --host 0.0.0.0",
    "build": "npm run build-app && npm run build-config",
    "build-app": "NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production node buildapp",
    "build-config": "NODE_ENV=production node buildconfig",
    "build-dev": "NODE_ENV=development node buildapp && NODE_ENV=development node buildconfig",
    "clean": "rm -rf dist coverage",
    "server": "node server",
    "lint": "eslint app test",
    "install-selenium": "selenium-standalone install --version=2.53.0",
    "build-docs": "./update-gh-pages.sh",
    "serve-docs": "gitbook serve",
    "update-translations": "i18next 'app/**/*.js' 'node_modules/tideline/{plugins,js}/**/*.js' 'node_modules/@tidepool/viz/{storiesDatatypes,storybookDatatypes,stories,src}/**/*.js' -c i18next-parser.config.json -o ."
  },
