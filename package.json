{
  "name": "blip",
  "version": "1.0.3",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D) built on top of the [Tidepool](http://tidepool.org/) platform.",
  "repository": {
    "url": "https://github.com/mdblp/blip"
  },
  "scripts": {
    "mrproper": "rm -rf dist node_modules coverage deploy server/dist server/node_modules server/.npmrc artifact_node.sh artifact_packaging.sh artifact_images.sh",
    "pretest-docker": "docker build --tag blip:test -f test/Dockerfile --build-arg npm_token=${nexus_token} .",
    "test-docker": "docker run blip:test",
    "pretest": "npm run lint",
    "test": "TZ=UTC NODE_ENV=test karma --log-level debug --reporters=mocha start",
    "test-watch": "TZ=UTC NODE_ENV=test karma start --no-single-run --reporters=mocha",
    "start-dev": "NODE_ENV=development USE_WEBPACK_DEV_SERVER=true ALLOW_SIGNUP_PATIENT=false ALLOW_PATIENT_CHANGE_EMAIL=false I18N_ENABLED=true ALLOW_PATIENT_CHANGE_PASSWORD=false HIDE_DONATE=true HIDE_DEXCOM_BANNER=true HIDE_UPLOAD_LINK=true CAN_SEE_PWD_LOGIN=true BRANDING='diabeloop' METRICS_SERVICE='disabled' HELP_LINK='disabled' webpack-dev-server --cache --colors --progress --port 3001 --host 0.0.0.0",
    "prebuild": "rm -rf dist server/dist deploy",
    "build": "npm run build-app && npm run build-config",
    "prebuild-app": "echo 'Building app'",
    "build-app": "NODE_ENV=production webpack",
    "build-config": "NODE_ENV=production node server/build-config.js",
    "prebuild-dev": "rm -rf dist server/dist",
    "build-dev": "NODE_ENV=development webpack --progress",
    "postbuild-dev": "NODE_ENV=development node server/build-config.js",
    "server": "node server/server.js dist",
    "lint": "eslint app test server",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "update-translations": "i18next 'app/**/*.js' 'node_modules/tideline/{plugins,js}/**/*.js' 'node_modules/@tidepool/viz/src/**/*.js' -c i18next-parser.config.json -o .",
    "build-soup": "release-helper gen-dep-report --deep-dep 'sundial,tideline,tidepool-platform-client,@tidepool/viz' doc/${npm_package_name}-${npm_package_version}-soup.md"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "morgan": "1.10.0",
    "@tidepool/viz": "github:mdblp/viz#dblp.0.13.0",
    "tideline": "github:mdblp/tideline#issue/ylp88-basics-sitechange-icon",
    "async": "3.2.0",
    "sundial": "github:mdblp/sundial#dblp.0.2.0",
    "tidepool-platform-client": "github:mdblp/platform-client#dblp.0.3.0",
    "bows": "1.7.2",
