{
  "name": "tideline",
  "version": "1.10.2",
  "description": "Tidepool's timeline data visualization",
  "repository": {
    "type": "git",
    "url": "https://github.com/tidepool-org/tideline.git"
  },
  "publishConfig": {
    "registry": "https://nexus.ci.diabeloop.eu/repository/your-loops-npm/"
  },
  "main": "js/index.js",
  "license": "BSD-2-Clause",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --optimize-minimize",
    "test": "TZ=UTC NODE_ENV=test karma start",
    "browser-tests": "karma start --browsers Chrome",
    "test-watch": "TZ=UTC NODE_ENV=test karma start --no-single-run --reporters=mocha",
    "start": "NODE_ENV=development webpack-dev-server -d --cache --colors --progress --content-base example --port 8081",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "lint": "NODE_ENV=test eslint js plugins example test"
  },
  "dependencies": {
    "bows": "1.7.2",
    "classnames": "2.2.6",
    "crossfilter2": "1.4.8",
    "d3": "3.5.17",
    "d3.chart": "0.3.0",
    "duration-js": "4.0.0",
    "i18next": "11.9.0",
    "lodash": "4.17.15",
    "moment-timezone": "0.5.31",
    "react": "15.6.2",
    "react-addons-pure-render-mixin": "15.6.2",
    "react-dom": "15.6.2",
    "react-sizeme": "2.5.2",
    "sundial": "github:mdblp/sundial#dblp.0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-rewire": "1.2.0",
    "babel-preset-react-app": "9.1.2",
    "chai": "4.2.0",
    "css-loader": "3.6.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-15": "1.4.1",
    "eslint": "7.3.1",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-lodash": "7.1.0",
    "eslint-plugin-moment-utc": "1.0.0",
    "eslint-plugin-react": "7.20.0",
    "file-loader": "6.0.0",
    "karma": "5.1.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "1.3.0",
    "karma-coverage": "2.0.2",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "4.0.2",
    "less": "3.11.3",
    "less-loader": "6.1.2",
    "mocha": "8.0.1",
    "postcss-calc": "7.0.2",
    "postcss-custom-properties": "9.1.1",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "15.6.2",
    "sinon": "9.0.2",
    "style-loader": "1.1.2",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
