{
  "name": "blip",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "mrproper": "rm -rf dist node_modules coverage server/dist server/node_modules packageMounts{@tidepool/viz,tideline,sundial,tidepool-platform-client}",
    "pretest-docker": "docker build --tag blip:test -f test/Dockerfile --build-arg npm_token=${nexus_token} .",
    "test-docker": "docker run blip:test",
    "test": "TZ=UTC NODE_ENV=test karma --log-level debug --reporters=mocha start",
    "pretest": "npm run lint",
    "browser-tests": "NODE_ENV=test karma start --browsers Chrome",
    "test-watch": "TZ=UTC NODE_ENV=test karma start --no-single-run --reporters=mocha",
    "start": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' webpack-dev-server --color --progress --port 3001 --host 0.0.0.0",
    "dblp": "NODE_ENV=development USE_WEBPACK_DEV_SERVER=true API_HOST='http://localhost:8009' SUPPORT_EMAIL_ADDRESS='yourloops@diabeloop.com' SUPPORT_WEB_ADDRESS='https://support.diabeloop.com/hc/categories/360001386093-YourLoops' ALLOW_SIGNUP_PATIENT=false ALLOW_PATIENT_CHANGE_EMAIL=false I18N_ENABLED=true ALLOW_PATIENT_CHANGE_PASSWORD=false HIDE_DONATE=true HIDE_DEXCOM_BANNER=true HIDE_UPLOAD_LINK=true BRANDING='diabeloop' METRICS_SERVICE='disabled' HELP_LINK='disabled' ASSETS_URL='https://url.com/' webpack-dev-server --cache --colors --progress --port 3001 --host 0.0.0.0",
    "prebuild": "rm -rf dist",
    "build": "npm run build-app && npm run build-config",
    "prebuild-app": "echo 'Building app'",
    "build-app": "NODE_ENV=production webpack --optimize-minimize",
    "build-config": "NODE_ENV=production node server/build-config.js",
    "prebuild-dev": "rm -rf dist server/dist",
    "build-dev": "NODE_ENV=development webpack --progress --optimize-minimize",
    "postbuild-dev": "NODE_ENV=development node server/build-config.js",
    "server": "node server/server.js",
    "lint": "eslint app test",
    "update-translations": "i18next 'app/**/*.js' 'node_modules/tideline/{plugins,js}/**/*.js' 'node_modules/@tidepool/viz/src/**/*.js' -c i18next-parser.config.json -o .",
    "build-soup": "release-helper gen-dep-report --deep-dep @tidepool/viz,tideline doc/${npm_package_name}-${npm_package_version}-soup.md"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
