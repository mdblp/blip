{
  "name": "tideline",
  "version": "1.12.1",
  "description": "Tidepool's timeline data visualization",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdblp/blip.git"
  },
  "publishConfig": {
    "registry": "https://nexus.ci.diabeloop.eu/repository/your-loops-npm/"
  },
  "main": "js/index.js",
  "license": "BSD-2-Clause",
  "scripts": {
    "mrproper": "rm -rf node_modules dist",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --optimize-minimize",
    "build-dev": "NODE_ENV=development webpack --progress",
    "test": "TZ=UTC NODE_ENV=test karma start",
    "browser-tests": "karma start --browsers Chrome",
    "test-watch": "TZ=UTC NODE_ENV=test karma start --no-single-run --reporters=mocha",
    "start": "NODE_ENV=development webpack-dev-server -d --cache --colors --progress --content-base example --port 8081",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "lint": "eslint js plugins example test"
  },
  "dependencies": {
    "bows": "1.7.2",
    "classnames": "2.2.6",
    "create-react-class": "^15.6.3",
    "crossfilter2": "1.4.8",
    "d3": "3.5.17",
    "d3.chart": "0.3.0",
    "duration-js": "4.0.0",
    "i18next": "11.9.0",
    "lodash": "^4.17.20",
    "moment-timezone": "0.5.31",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-sizeme": "2.6.12",
    "sundial": "file:../sundial"
  }
}
