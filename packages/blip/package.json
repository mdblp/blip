{
  "name": "blip",
  "version": "1.4.1",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D) built on top of the [Tidepool](http://tidepool.org/) platform.",
  "repository": {
    "url": "https://github.com/mdblp/blip"
  },
  "scripts": {
    "mrproper": "rm -rf dist node_modules coverage deploy artifact_node.sh artifact_packaging.sh artifact_images.sh",
    "clean": "rm -rf dist deploy coverage node_modules/.cache",
    "pretest-docker": "docker build --tag blip:test -f test/Dockerfile --build-arg npm_token=${nexus_token} .",
    "test-docker": "docker run blip:test",
    "test": "TZ=UTC NODE_ENV=test karma --log-level debug start",
    "test-watch": "TZ=UTC NODE_ENV=test karma start --no-single-run --reporters=mocha",
    "start-dev": "NODE_ENV=development USE_WEBPACK_DEV_SERVER=true ALLOW_SIGNUP_PATIENT=false ALLOW_PATIENT_CHANGE_EMAIL=false I18N_ENABLED=true ALLOW_PATIENT_CHANGE_PASSWORD=false HIDE_DONATE=true HIDE_DEXCOM_BANNER=true HIDE_UPLOAD_LINK=true CAN_SEE_PWD_LOGIN=true BRANDING='diabeloop' METRICS_SERVICE='disabled' HELP_LINK='disabled' webpack-dev-server --cache --colors --port 3001 --host 0.0.0.0",
    "prebuild": "npm run clean",
    "build": "bash build.sh",
    "prebuild-dev": "npm run clean",
    "build-dev": "NODE_ENV=development webpack",
    "postbuild-dev": "NODE_ENV=development node ../../server/build-config.js",
    "server": "NODE_ENV=development node ../../server/server.js dist",
    "lint": "eslint app test",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "update-translations": "i18next 'app/**/*.js' 'node_modules/tideline/{plugins,js}/**/*.js' '../viz/src/**/*.js' -c i18next-parser.config.json -o ."
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "dependencies": {
    "async": "3.2.0",
    "body-parser": "1.19.0",
    "bows": "1.7.2",
    "browser-locale": "1.0.3",
    "classnames": "2.2.6",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "custom-protocol-detection": "1.0.1",
    "express": "4.17.1",
    "fixed-data-table-2": "0.8.15",
    "github-api": "^3.3.0",
    "helmet": "3.22.0",
    "history": "2.1.2",
    "i18next": "11.9.0",
    "lodash": "^4.17.15",
    "moment-timezone": "0.5.31",
    "morgan": "1.10.0",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "15.6.2",
    "react-addons-create-fragment": "15.6.2",
    "react-addons-perf": "15.6.0-rc.1",
    "react-addons-pure-render-mixin": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-addons-update": "15.6.2",
    "react-cookie-consent": "3.0.0",
    "react-dom": "15.6.2",
    "react-hot-loader": "4.3.10",
    "react-i18next": "7.13.0",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "3.0.0",
    "react-scroll": "1.7.10",
    "react-select": "~2.3.0",
    "react-window-size-listener": "1.2.1",
    "redux": "3.5.2",
    "redux-cache": "0.3.0",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "2.6.1",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "0.13.5",
    "reselect": "3.0.1",
    "sundial": "file:../sundial",
    "tideline": "file:../tideline",
    "tidepool-platform-client": "file:../platform-client",
    "tidepool-viz": "file:../viz"
  }
}
