{
  "name": "tidepool-viz",
  "version": "0.15.1",
  "description": "Tidepool data visualization for diabetes device data.",
  "keywords": [
    "data visualization"
  ],
  "main": "src/index.js",
  "scripts": {
    "mrproper": "rm -rf dist coverage node_modules",
    "browser-tests": "NODE_ENV=test karma start --browsers Chrome",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --config package.config.js",
    "prebuild-dev": "rm -rf dist coverage",
    "build-dev": "NODE_ENV=development webpack --progress --config package.config.js",
    "clean": "rm -rf dist coverage",
    "lint": "eslint src/ test/ *.js",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "start": "NODE_ENV=development webpack --config package.config.js --watch",
    "test": "TZ=UTC NODE_ENV=test karma start",
    "test-watch": "TZ=UTC NODE_ENV=test karma start --no-single-run --reporters=mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdblp/blip.git"
  },
  "publishConfig": {
    "registry": "https://nexus.ci.diabeloop.eu/repository/your-loops-npm/"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/tidepool-org/viz/issues"
  },
  "homepage": "https://github.com/tidepool-org/viz#readme",
  "files": [
    "src/",
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "blob-stream": "0.1.3",
    "bows": "1.7.2",
    "classnames": "2.2.6",
    "crossfilter2": "1.4.8",
    "d3-array": "1.2.4",
    "d3-format": "1.3.2",
    "d3-scale": "2.1.2",
    "d3-shape": "1.2.2",
    "d3-time": "1.0.10",
    "d3-time-format": "2.1.3",
    "gsap": "3.2.6",
    "i18next": "11.9.0",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.20",
    "moment-timezone": "0.5.31",
    "parse-svg-path": "0.1.2",
    "pdfkit": "0.11.0",
    "prop-types": "15.6.2",
