{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "globalThis": true,
    "AppConfig": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "moment-utc",
    "react"
  ],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "max-lines": "error",
    "semi": "error"
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.13.1"
    }
  }
}
