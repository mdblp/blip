{
  "name": "yourloops",
  "version": "3.0.4-beta",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D).",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdblp/blip"
  },
  "author": {
    "name": "Diabeloop",
    "url": "https://www.diabeloop.com/"
  },
  "license": "BSD-2-Clause",
  "scripts": {
    "mrproper": "rm -rf node_modules dist",
    "clean": "rm -rf dist",
    "build": "npm run build-prod",
    "prebuild-prod": "npm run clean",
    "build-prod": "NODE_ENV=production webpack --mode=production",
    "prebuild-dev": "npm run clean",
    "build-dev": "NODE_ENV=development webpack --mode=development",
    "start-dev": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' USE_WEBPACK_DEV_SERVER=true webpack serve --host 0.0.0.0",
    "test": "npm run unit-test && npm run integration-test",
    "unit-test": "jest --config=test/unit/jest.config.js",
    "integration-test": "jest --config=test/integration/jest.config.js",
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.0.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.1.0",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.11.14",
    "axios": "^1.3.4",
    "bows": "^1.7.2",
    "clsx": "^1.1.1",
    "core-js": "^3.27.2",
    "dayjs": "^1.11.7",
    "emoji-picker-react": "^4.4.8",
    "html-react-parser": "^3.0.8",
    "i18next": "^22.4.9",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "moment-timezone": "^0.5.40",
    "react": "17.0.2",
    "react-device-detect": "^2.2.2",
    "react-dom": "17.0.2",
    "react-i18next": "^12.1.5",
    "react-idle-timer": "^5.5.2",
    "react-router-dom": "^6.8.2",
    "tss-react": "^4.6.0",
    "uuid": "^9.0.0"
  }
}
