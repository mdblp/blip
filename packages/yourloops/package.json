{
  "name": "yourloops",
  "version": "2.0.0-alpha1",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D).",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdblp/blip"
  },
  "author": {
    "name": "Diabeloop",
    "url": "https://www.diabeloop.com/"
  },
  "license": "BSD-2-Clause",
  "scripts": {
    "mrproper": "rm -rf node_modules dist",
    "clean": "rm -rf dist",
    "lint": "eslint app lib models pages",
    "build": "npm run build-prod",
    "prebuild-prod": "npm run clean",
    "build-prod": "webpack --mode=production",
    "prebuild-dev": "npm run clean && npm run build-blip-dev",
    "build-dev": "webpack --mode=development --progress",
    "postbuild-dev": "mv -iv ../blip/dist/* dist/",
    "build-blip-dev": "cd ../..; npm run build-iframe-dev",
    "prebuild-dev-only": "npm run clean",
    "build-dev-only": "webpack --mode=development --progress",
    "postbuild-dev-only": "cp -iv ../blip/dist/* dist/",
    "server": "NODE_ENV=development node ../../server/server.js dist",
    "start-dev": "NODE_ENV=development webpack-dev-server --cache --colors --progress --port 3001 --host 0.0.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reach/router": "^1.3.4",
    "bows": "^1.7.2",
    "browser-locale": "^1.0.3",
    "fontsource-roboto": "^3.0.3",
    "i18next": "^19.7.0",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.31",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/i18next": "^13.0.0",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/reach__router": "^1.3.5",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "css-loader": "^4.2.2",
    "eslint": "^7.7.0",
    "eslint-plugin-moment-utc": "^1.0.0",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-subresource-integrity": "^1.4.1"
  }
}
