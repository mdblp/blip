{
  "name": "yourloops",
  "version": "3.0.1-RC1",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D).",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdblp/blip"
  },
  "author": {
    "name": "Diabeloop",
    "url": "https://www.diabeloop.com/"
  },
  "license": "BSD-2-Clause",
  "scripts": {
    "mrproper": "rm -rf node_modules dist",
    "clean": "rm -rf dist",
    "build": "npm run build-prod",
    "prebuild-prod": "npm run clean",
    "build-prod": "NODE_ENV=production webpack --mode=production",
    "prebuild-dev": "npm run clean",
    "build-dev": "NODE_ENV=development webpack --mode=development",
    "start-dev": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' USE_WEBPACK_DEV_SERVER=true webpack-dev-server --cache --color --port 3001 --host 0.0.0.0",
    "test": "jest --config=test/jest.config.js",
    "lint": "eslint . --ext .ts,.tsx --quiet",
    "lint-fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.1.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "latest",
    "@material-ui/styles": "^4.11.4",
    "axios": "^0.25.0",
    "blip": "file:../blip",
    "bows": "^1.7.2",
    "clsx": "^1.1.1",
    "core-js": "^3.9.1",
    "dayjs": "^1.10.7",
    "emoji-picker-react": "^3.5.1",
    "i18next": "^19.8.4",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.31",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.2"
  }
}
