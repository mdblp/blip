{
  "name": "yourloops",
  "version": "2.0.5",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D).",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdblp/blip"
  },
  "author": {
    "name": "Diabeloop",
    "url": "https://www.diabeloop.com/"
  },
  "license": "BSD-2-Clause",
  "scripts": {
    "mrproper": "rm -rf node_modules dist",
    "clean": "rm -rf dist",
    "build": "npm run build-prod",
    "prebuild-prod": "npm run clean",
    "build-prod": "NODE_ENV=production webpack --mode=production",
    "prebuild-dev": "npm run clean",
    "build-dev": "NODE_ENV=development webpack --mode=development",
    "start-dev": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' USE_WEBPACK_DEV_SERVER=true webpack-dev-server --cache --color --port 3001 --host 0.0.0.0",
    "test": "LANG=C TZ=UTC NODE_ENV='test' NODE_OPTIONS='--max-old-space-size=4096' karma --single-run --log-level debug start",
    "test-debug": "LANG=C TZ=UTC NODE_ENV='test' DEBUG_UNIT_TEST='true' NODE_OPTIONS='--max-old-space-size=4096' karma --single-run=false --log-level debug start --auto-watch"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.1.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@material-ui/lab": "latest",
    "axios": "^0.25.0",
    "blip": "file:../blip",
    "bows": "^1.7.2",
    "clsx": "^1.1.1",
    "core-js": "^3.9.1",
    "dayjs": "^1.10.7",
    "i18next": "^19.8.4",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.31",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.2",
    "zxcvbn": "^4.4.2"
  }
}
